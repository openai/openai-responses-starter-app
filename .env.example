# Example environment variables for TacticDev Gen Intel
# Copy this file to `.env` locally and fill in your secrets before running locally.

# App configuration
NEXT_PUBLIC_APP_NAME="TacticDev Gen Intel"
NEXT_PUBLIC_SITE_ORIGIN="https://gen.tacticdev.com"

# OpenAI / Responses API
OPENAI_API_KEY="your-openai-api-key"
RESPONSES_MODEL="gpt-5.2"
RESPONSES_DEVELOPER_PROMPT="Optional: override developer prompt"

# Google OAuth
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"
GOOGLE_REDIRECT_URI="https://gen.tacticdev.com/api/google/callback"

# GitHub OAuth
GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"
GITHUB_REDIRECT_URI="https://gen.tacticdev.com/api/github/callback"

# Optional: vector store defaults
DEFAULT_VECTOR_STORE_ID=""
DEFAULT_VECTOR_STORE_NAME="core-knowledge"

# Admin / app secrets
ADMIN_PASSWORD="a-strong-password"